                <% for(var b of beauty ){ %>
                  <div class="col m6 s12">
                      <div class="">
                          <a href=""> <img width="100%" class="image responsive-img" src="/uploads/beauty/<%=b.imageUrl%>" alt=""> </a>
                      </div>
                  </div>
                  <% } %>
            </div> -->
            <% if(isAuthenticated === true) {%>
            <div class="row">
                <div class="col m8 offset-m2 s12">
                    <form action="/beauty/create" method="POST" enctype="multipart/form-data">
                        <div class="form-group">
                            <div class="input-field col s12">
                                <input name="model_name" type="text" class="validate"/>
                                <label for="model_name">Event</label>
                            </div>
                            <label>Select an Image</label>
                            <div class="file-field input-field">
                                <div class="btn grey small">
                                    <span>Browse</span>
                                    <input type="file" name="imageUrl">
                                </div>
                                <div class="file-path-wrapper">
                                    <input class="file-path validate" type="text"
                                    placeholder="File Upload">
                                </div>
                            </div>
                         <input type="submit" class="btn" name="" value="Submit"/>
                     </form>
                 </div>
            </div>
        </div>
        <% } %>
</div>
<%- include('../partials/foot') %>
