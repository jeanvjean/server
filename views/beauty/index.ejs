<%- include('../partials/head') %>

<div class="container mc">
            <div class="row center">
                <% beauty.forEach(function(beauty) { %>
                  <div class="col m6 s12">
                      <div class="">
                           <img width="100%" class="image responsive-img" src="/uploads/beauty/<%=beauty.imageUrl%>" alt="">
                      </div>
                  </div>
                  <% }) %>
            </div>
